   //Run over Online Compiler
//Task:You have to print the character,ch, in the first line. Then print s in next line. In the last line print the sentence, sen .

#include <stdio.h>

int main() 
{
    char ch;
     char s[100];
    char sen[100];
     
    scanf("%c" , &ch);
    printf("%c \n",ch);

    scanf("%s" , &s);
    printf("%s \n",s);

    scanf("%s" , &sen);
    printf("%s To C!! ",sen);

    return 0;
}
